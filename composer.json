{
    "name": "gurucomkz/eagerloading",
    "description": "Eager loading for SilverStripe 4",
    "type": "silverstripe-vendormodule",
    "keywords": [
        "silverstripe"
    ],
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Sergey Shevchenko",
            "role": "Developer"
        }
    ],
    "require": {
        "silverstripe/framework": "^4.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "jdorn/sql-formatter": "1.3.x-dev",
        "phpunit/php-code-coverage": "dev-4.0-dev as 4.0.4",
        "tractorcow/silverstripe-proxy-db": "^0.1.0"
    },
    "autoload": {
        "psr-4": {
            "Gurucomkz\\EagerLoading\\": "src/",
            "Gurucomkz\\EagerLoading\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/elyobo/php-code-coverage.git"
        }
    ],
    "extra": {
        "expose": [
            "client"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
